<!doctype html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1.0"
    />
    {% block title %}{{ page.seoTitle ?? page.title }}{% endblock %}
    {% block meta_description %}{{ page.seoDescription }}{% endblock %}

    {% block stylesheets %}
      {{ encore_entry_link_tags('app') }}
    {% endblock %}
  </head>
  <body>
    <h1 class="text-2xl font-bold underline text-red-500">
      {{ page.title }}
    </h1>
  </body>
</html>